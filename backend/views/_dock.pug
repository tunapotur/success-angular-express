-
  const link = user ? "/add-success" : "/login";
  

.header-dock.dock
  div(class="justify-self-start")
    if url !=="/"
      svg.dock__icon(id="go-back")
        use(xlink:href="../img/icons.svg#icon-arrow-thick-left")

  div(class="justify-self-center")
    if url!=="/add-success"
      a(href=`${link}`)
        svg.dock__icon 
          use(xlink:href="../img/icons.svg#icon-square-plus-solid")
    else
      h2.dock__text ADD SUCCESS
  
  div(class="justify-self-end")
    if owner
      a(id="edit-success" href=`/edit-success/${success._id}`)
        svg.dock__icon 
          use(xlink:href="../img/icons.svg#icon-document-edit")
